{
  "ConnectionStrings": { },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Space": {
    "ServerUrl": "https://{organization}.jetbrains.space",
    "ClientId": "{client-id}",
    "ClientSecret": "{client-secret}",
    "ValidatePayloadSignature": true,
    "EndpointSigningKey": "{endpoint-signing-key}",
    "ValidatePayloadVerificationToken": true,
    "EndpointVerificationToken": "{endpoint-verification-token}"
  }
}