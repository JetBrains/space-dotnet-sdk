@using Microsoft.AspNetCore.Identity

<ul class="navbar-nav">
@if (User.Identity.IsAuthenticated)
{
    <li class="nav-item">
        <img src="@User.Identity.GetClaimValue("urn:space:smallAvatar")" width="32" height="32" />
        @User.Identity.GetClaimValue("urn:space:fullName")
    </li>
}
</ul>